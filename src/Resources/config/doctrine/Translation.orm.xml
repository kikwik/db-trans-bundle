<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Kikwik\DbTransBundle\Entity\Translation" table="kw_translation">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="domain" type="string" column="domain"/>
        <field name="locale" type="string" column="locale"/>
        <field name="messageId" type="string" column="messageId"/>
        <field name="message" type="text" column="message" nullable="true"/>

    </entity>

</doctrine-mapping>